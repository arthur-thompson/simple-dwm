# Reload functions
super + Escape
	"$XDG_CONFIG_HOME"/sxhkd/functions

# brightness down
XF86MonBrightnessDown
	brightnessctl s 5%-

# brightness up
XF86MonBrightnessUp
	brightnessctl s +5%

# brightness down
super + XF86MonBrightnessDown
	brightnessctl s 1%-

# brightness up
super + XF86MonBrightnessUp
	brightnessctl s +1%

#Volume Control
XF86AudioLowerVolume
	amixer set Master 5%-
XF86AudioRaiseVolume
	amixer set Master 5%+
XF86AudioMute
    amixer set Master toggle
